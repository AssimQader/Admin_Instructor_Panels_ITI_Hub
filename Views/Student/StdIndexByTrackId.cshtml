@model IEnumerable<Admin_Panel_ITI.Models.Student>

@{
    ViewData["Title"] = "Students by Track";
}

<h1>Students by Track</h1>

<table class="table">
    <thead>
        <tr>
            <th>Student ID</th>
            <th>Full Name</th>
            <th>Is Graduated</th>
            <th>Creation Date</th>
            <th>Intake</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.AspNetUserID</td>
                <td>@student.AspNetUser.FullName</td>
                <td>@(student.IsGraduated ? "Yes" : "No")</td>
                <td>@student.CreationDate.ToString("yyyy-MM-dd")</td>
                <td>@student.Intake.Name</td>
            </tr>
        }
    </tbody>
</table>
