@model IEnumerable<Admin_Panel_ITI.Models.Student>

<h1>Students by Intake</h1>

<table class="table">
    <thead>
        <tr>
            <th>Student ID</th>
            <th>Full Name</th>
            <th>Username</th>
            <th>Is Graduated</th>
            <th>Creation Date</th>
            <th>Track</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.FullName</td>
                <td>@student.UserName</td>
                <td>@(student.IsGraduated ? "Yes" : "No")</td>
                <td>@student.CreationDate.ToString("yyyy-MM-dd")</td>
                <td>@student.Track.Name</td>
            </tr>
        }
    </tbody>
</table>
